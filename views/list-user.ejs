<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>List User</title>
  </head>
  <body>
    <h1>List User</h1>
    <ul>
      <% users.forEach(user => { %>
      <li>
        <ul>
          <li>Name: <%= user.name %></li>
          <li>Age: <%= user.age %></li>
          <li>About: <%= user.about %></li>
        </ul>
      </li>
      <% }); %>
    </ul>
  </body>
</html>
